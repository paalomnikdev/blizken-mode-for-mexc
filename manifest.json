{
    "manifest_version": 3,
    "author": "Ivan Chornyi",
    "name": "Blizken mode for MEXC",
    "version": "0.4.2",
    "description": "This extension will make PNL values white.",
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [{
      "js": ["content.js"],
      "matches": ["https://futures.mexc.com/exchange/*"]
    }],
    "permissions": [
      "storage"
    ]
  }